<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ChangePrimitiveFXMLController">
    <children>
        <VBox>
            <children>
                <HBox layoutX="15.0" layoutY="15.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Pane HBox.hgrow="ALWAYS" />
                        <Label prefHeight="40.0" text="Change primitive" textAlignment="CENTER" />
                        <Pane HBox.hgrow="ALWAYS" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox layoutX="10.0" layoutY="50.0">
                    <children>
                        <Label prefHeight="40.0" prefWidth="180.0" text="X coordinate" />
                        <TextField prefHeight="40.0" prefWidth="220.0" fx:id="xCoordText" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox layoutX="15.0" layoutY="115.0">
                    <children>
                        <Label prefHeight="40.0" prefWidth="180.0" text="Y coordinate" />
                        <TextField fx:id="yCoordText" prefHeight="40.0" prefWidth="220.0" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox fx:id="circleBox" layoutX="10.0" layoutY="160.0">
                    <children>
                        <Label prefHeight="40.0" prefWidth="180.0" text="Radius" />
                        <TextField fx:id="radiusText" prefHeight="40.0" prefWidth="220.0" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox fx:id="lengthBox" layoutX="10.0" layoutY="160.0">
                    <children>
                        <Label prefHeight="40.0" prefWidth="180.0" text="Length" />
                        <TextField fx:id="lengthText" prefHeight="40.0" prefWidth="220.0" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox fx:id="widthBox" layoutX="10.0" layoutY="160.0">
                    <children>
                        <Label prefHeight="40.0" prefWidth="180.0" text="Width" />
                        <TextField fx:id="widthText" prefHeight="40.0" prefWidth="220.0" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox>
                    <children>
                        <Pane layoutX="90.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#drawOnCanva" prefHeight="25.0" prefWidth="70.0" text="OK">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </Button>
                        <Pane HBox.hgrow="ALWAYS" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
