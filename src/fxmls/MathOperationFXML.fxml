<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MathOperationFXMLController">
   <children>
      <VBox>
         <children>
            <HBox layoutX="15.0" layoutY="15.0" VBox.vgrow="ALWAYS">
               <children>
                  <Pane HBox.hgrow="ALWAYS" />
                  <Label prefHeight="40.0" text="Math operation" textAlignment="CENTER" />
                  <Pane HBox.hgrow="ALWAYS" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
            <HBox>
               <children>
                  <Label prefHeight="40.0" prefWidth="180.0" text="Operation:" />
                  <ComboBox fx:id="mathOperation" prefHeight="40.0" prefWidth="220.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
            <HBox layoutX="10.0" layoutY="50.0">
               <children>
                  <Label prefHeight="40.0" prefWidth="180.0" text="R" />
                  <TextField fx:id="rField" prefHeight="40.0" prefWidth="220.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
            <HBox layoutX="15.0" layoutY="115.0">
               <children>
                  <Label prefHeight="40.0" prefWidth="180.0" text="G" />
                  <TextField fx:id="gField" prefHeight="40.0" prefWidth="220.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
            <HBox layoutX="15.0" layoutY="165.0">
               <children>
                  <Label prefHeight="40.0" prefWidth="180.0" text="B" />
                  <TextField fx:id="bField" prefHeight="40.0" prefWidth="220.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
            <HBox>
               <children>
                  <Pane layoutX="90.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
                  <Button fx:id="doButton" cancelButton="true" mnemonicParsing="false" onAction="#dismiss" prefHeight="25.0" prefWidth="70.0" text="OK">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Pane HBox.hgrow="ALWAYS" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
